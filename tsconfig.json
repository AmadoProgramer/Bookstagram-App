{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".vscode/.expo/types/**/*.ts",
    "expo-env.d.ts",
    "declarations.d.ts",
    ".expo/types/**/*.ts"
  ]
}
